{
  "name": "twilio-style",
  "version": "1.3.0",
  "description": "Twilio's JavaScript linting style",
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "styleguide",
    "twilio"
  ],
  "homepage": "https://github.com/twilio-labs/twilio-style#readme",
  "bugs": {
    "url": "https://github.com/twilio-labs/twilio-style/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilio-labs/twilio-style.git"
  },
 "rules": {
 "linebreak-style": 0,
 "global-require": 0,
 "eslint linebreak-style": [0, "error", "windows"],
  
  "license": "MIT",
  "author": "Twilio @twilio",
  "scripts": {
    "bootstrap": "lerna bootstrap --no-ci",
    "postinstall": "npm run bootstrap",
    "test": "jest --coverage --color",
    "test:watch": "jest --watchAll --color"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "lerna": "^3.16.4"
  }
}
